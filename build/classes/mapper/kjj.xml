<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.Mapper_kjj">
	<cache />
	<insert id="registerSell" parameterType="Sell">
		INSERT INTO Sell VALUES(seq_SellNo_Sell.nextval, '1234', #{itemName}, #{categoryNo}, #{keyword1}, #{keyword2}, #{keyword3}, 
		<foreach collection="type" item="item" open="'" close="'" separator=" ">
			${item}
		</foreach>
		, #{region}, #{itemCondition}, #{price}, #{picture}, '등록')
	</insert>

</mapper>















